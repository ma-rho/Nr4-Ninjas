rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /djs/{allPaths=**} {
      allow read;
      allow write: if request.auth != null;
    }

    match /merch/{allPaths=**} {
     allow read;
     allow write: if request.auth != null;
   }

   match /events/{allPaths=**} {
     allow read;
     allow write: if request.auth != null;
   }

   match /photos/{allPaths=**} {
     allow read;
     allow write: if request.auth != null;
   }
  }
}
